{
  "name": "cra",
  "homepage": "/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "3.3.7",
    "dotenv": "^16.0.0",
    "font-awesome": "^4.7.0",
    "node": "^21.6.2",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.2.3",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "styled-components": "^6.1.11",
    "typescript": "4.9.4",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev envvar",
    "build": "webpack --config webpack/webpack.config.js --env env=prod envvar",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "prod": "webpack --config webpack/webpack.config.js --env env=prod envvar && serve build",
    "generate": "plop"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.17.9",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@sentry/webpack-plugin": "^1.18.8",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.12",
    "@types/node": "18.11.18",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "6",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "^12.3.7",
    "plop": "^4.0.1",
    "prettier": "^2.6.2",
    "react-refresh": "^0.12.0",
    "style-loader": "^3.3.1",
    "ts-jest": "29.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
